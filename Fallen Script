local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid") or nil

local defaultSpeed = 16
if humanoid then
	defaultSpeed = humanoid.WalkSpeed
end

local Section = Tab:CreateSection("–î–≤–∏–∂–µ–Ω–∏–µ") -- –ù–∞–∑–≤–∞–Ω–∏–µ —Å–µ–∫—Ü–∏–∏

-- –ü–æ–ª–µ –≤–≤–æ–¥–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏
local WalkSpeedInput = Tab:CreateInput({
	Name = "–£—Å—Ç–∞–Ω–æ–≤–∏—Ç—å WalkSpeed",
	PlaceholderText = tostring(defaultSpeed),
	RemoveTextAfterFocusLost = true,
	Callback = function(Text)
	if Text == "" then return end
		humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			local value = tonumber(Text)
			if value then
				humanoid.WalkSpeed = value
				if WalkSpeedInput then
					WalkSpeedInput:Set(tostring(value))
				end
				Rayfield:Notify({
					Title = "WalkSpeed –∏–∑–º–µ–Ω—ë–Ω",
					Content = "–ù–æ–≤–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å: " .. value,
					Duration = 3
				})
			else
				Rayfield:Notify({
					Title = "–¢–µ–∫—É—â–µ–µ –∑–Ω–∞—á–µ–Ω–∏–µ WalkSpeed",
					Content = "–°–µ–π—á–∞—Å: " .. humanoid.WalkSpeed,
					Duration = 3
				})
			end
		end
	end
})

-- –ö–Ω–æ–ø–∫–∞ —Å–±—Ä–æ—Å–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏
Tab:CreateButton({
	Name = "–°–±—Ä–æ—Å–∏—Ç—å –¥–æ —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–æ–π —Å–∫–æ—Ä–æ—Å—Ç–∏",
	Callback = function()
		humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.WalkSpeed = defaultSpeed
			Rayfield:Notify({
				Title = "–°–∫–æ—Ä–æ—Å—Ç—å —Å–±—Ä–æ—à–µ–Ω–∞",
				Content = "–£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞ —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–∞—è: " .. tostring(defaultSpeed),
				Duration = 3
			})
		end
	end
})

local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid") or nil

local defaultJumpPower = 50
if humanoid then
	defaultJumpPower = humanoid.JumpPower
end

local Section = Tab:CreateSection("–ü—Ä—ã–∂–∫–∏")

-- –ü–æ–ª–µ –≤–≤–æ–¥–∞ JumpPower
local JumpPowerInput = Tab:CreateInput({
	Name = "–£—Å—Ç–∞–Ω–æ–≤–∏—Ç—å JumpPower",
	PlaceholderText = tostring(defaultJumpPower),
	RemoveTextAfterFocusLost = true,
	Callback = function(Text)
		if Text == "" then return end
		local value = tonumber(Text)
		humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if humanoid and value then
			humanoid.JumpPower = value
			Rayfield:Notify({
				Title = "JumpPower –∏–∑–º–µ–Ω—ë–Ω",
				Content = "–ù–æ–≤–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ: " .. value,
				Duration = 3
			})
		else
			Rayfield:Notify({
				Title = "–û—à–∏–±–∫–∞",
				Content = "–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ",
				Duration = 2
			})
		end
	end
})

-- –ö–Ω–æ–ø–∫–∞ —Å–±—Ä–æ—Å–∞ JumpPower
Tab:CreateButton({
	Name = "–°–±—Ä–æ—Å–∏—Ç—å JumpPower –¥–æ —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–æ–≥–æ",
	Callback = function()
		humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.JumpPower = defaultJumpPower
			Rayfield:Notify({
				Title = "JumpPower —Å–±—Ä–æ—à–µ–Ω",
				Content = "–£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–æ–µ: " .. tostring(defaultJumpPower),
				Duration = 3
			})
		end
	end
})

local Section = Tab:CreateSection("FPS BOOST")

-- ‚¨á –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è –æ—Ç–∫–ª—é—á–µ–Ω–∏—è —Ä–µ–Ω–¥–µ—Ä–∞
local renderingDisabled = false
local originalCameraType = workspace.CurrentCamera.CameraType
local originalCFrame = workspace.CurrentCamera.CFrame

-- ‚¨á Toggle –¥–ª—è –æ—Ç–∫–ª—é—á–µ–Ω–∏—è 3D —Ä–µ–Ω–¥–µ—Ä–∞
Tab:CreateToggle({
    Name = "üîª –û—Ç–∫–ª—é—á–∏—Ç—å 3D —Ä–µ–Ω–¥–µ—Ä (–ø–æ–≤—ã—à–µ–Ω–∏–µ FPS)",
    CurrentValue = false,
    Callback = function(Value)
        renderingDisabled = Value

        local cam = workspace.CurrentCamera

        if Value then
            originalCFrame = cam.CFrame
            originalCameraType = cam.CameraType

            cam.CameraType = Enum.CameraType.Scriptable
            cam.CFrame = CFrame.new(0, 1e9, 0) -- –£–≤–æ–¥–∏–º "–≤ –±–µ–ª—ã–π –∫–æ—Å–º–æ—Å"
            
            -- –û—Ç–∫–ª—é—á–∏–º –≤–∏–∑—É–∞–ª—å–Ω—ã–µ —ç—Ñ—Ñ–µ–∫—Ç—ã (–ø–æ –∂–µ–ª–∞–Ω–∏—é)
            game:GetService("Lighting").FogEnd = 9e9
            game:GetService("Lighting").Brightness = 0

            Rayfield:Notify({
                Title = "3D –†–µ–Ω–¥–µ—Ä–∏–Ω–≥ –æ—Ç–∫–ª—é—á—ë–Ω",
                Content = "FPS –¥–æ–ª–∂–µ–Ω –∑–Ω–∞—á–∏—Ç–µ–ª—å–Ω–æ –≤—ã—Ä–∞—Å—Ç–∏",
                Duration = 3
            })
        else
            cam.CameraType = originalCameraType
            cam.CFrame = originalCFrame

            -- –í–æ—Å—Å—Ç–∞–Ω–æ–≤–∏–º —ç—Ñ—Ñ–µ–∫—Ç—ã (–º–æ–∂–Ω–æ –Ω–∞—Å—Ç—Ä–æ–∏—Ç—å –ø–æ–¥ —Å–µ–±—è)
            game:GetService("Lighting").FogEnd = 1000
            game:GetService("Lighting").Brightness = 2

            Rayfield:Notify({
                Title = "3D –†–µ–Ω–¥–µ—Ä–∏–Ω–≥ –≤–∫–ª—é—á—ë–Ω",
                Content = "–°—Ü–µ–Ω–∞ —Å–Ω–æ–≤–∞ –≤–∏–¥–Ω–∞",
                Duration = 2
            })
        end
    end
})

Rayfield:LoadConfiguration()
